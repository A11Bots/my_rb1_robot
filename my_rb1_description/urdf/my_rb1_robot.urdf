<?xml version="1.0" ?>

<robot name="Rb_1">
    <gazebo reference="base_link">
        <material>Gazebo/Black</material>
    </gazebo>
	<!--Link - base_link -->
	<link name="base_link">
		<inertial>
			<mass value="21" />
			<origin xyz="0 0 0.15" rpy="0 0 0" />
			<inertia ixx="0.486" ixy="0" ixz="0" iyy="0.486" iyz="0" izz="0.656" />
		</inertial>

		<collision>
			<geometry>
				<cylinder length="0.3" radius="0.25"/>
			</geometry>
		</collision>

		<visual>
			<geometry>
				<cylinder length="0.3" radius="0.25"/>
			</geometry>
			<material name="DarkBlue">
				<color rgba="0.2 0.2 0.4 1" />
			</material>
		</visual>
	</link>

	<!-- Joint - base_link / left wheel -->
	<joint name="joint_baselink_leftwheel" type="continuous">
		<origin rpy="0 0 0" xyz="0 -0.2 -0.15" />
		<child link="left_wheel" />
		<parent link="base_link" />
		<axis rpy="0 0 0" xyz="0 -0.2 0" />
		<limit effort="10000" velocity="1000" />
		<joint_properties damping="1.0" friction="1.0" />
	</joint>
	<!-- Link - left_wheel -->
	<link name="left_wheel">
		<inertial>
			<mass value="1" />
			<origin xyz="0 0 0" rpy="0 0 0" />
			<inertia ixx="0.0019" ixy="0" ixz="0" iyy="0.0019" iyz="0" izz="0.0005"  />
		</inertial>

		<!-- Tires -->
		<collision>
			<origin rpy="1.5707 0 0" xyz="0 0 0" />
			<geometry>
				<cylinder length="0.05" radius="0.05"/>	
			</geometry>
		</collision>
		<visual>
			<origin rpy="1.5707 0 0" xyz="0 0 0" /> 
			<geometry>
				<cylinder length="0.05" radius="0.05"/>	
			</geometry>
			<material name="Red">
				<color rgba="1 0 0 1" />
			</material>
		</visual>
    </link>

    <!-- Joint - base_link / right wheel -->
	<joint name="joint_baselink_rightwheel" type="continuous">
		<origin rpy="0 0 0" xyz="0 0.2 -0.15" />
		<child link="right_wheel" />
		<parent link="base_link" />
		<axis rpy="0 0 0" xyz="0 0.2 0" />
		<limit effort="10000" velocity="1000" />
		<joint_properties damping="1.0" friction="1.0" />
	</joint>
	<!-- Link - right_wheel -->
	<link name="right_wheel">
		<inertial>
			<mass value="1" />
			<origin xyz="0 0 0" rpy="0 0 0" />
			<inertia ixx="0.0019" ixy="0" ixz="0" iyy="0.0019" iyz="0" izz="0.0005"  />
		</inertial>

		<!-- Tires -->
		<collision>
			<origin rpy="1.5707 0 0" xyz="0 0 0" />
			<geometry>
				<cylinder length="0.05" radius="0.05"/>	
			</geometry>
		</collision>
		<visual>
			<origin rpy="1.5707 0 0" xyz="0 0 0" /> 
			<geometry>
				<cylinder length="0.05" radius="0.05"/>	
			</geometry>
			<material name="Red">
				<color rgba="1 0 0 1" />
			</material>
		</visual>
    </link>

    <!-- Joint - caster wheel -->
    <joint name="joint_chassis_caster_wheel" type="fixed">
        <origin rpy="0 0 0" xyz="-0.1 0 -0.15" />
        <child link="back_caster" />
        <parent link="base_link" />
        <joint_properties damping="1.0" friction="0.0" />
    </joint>

    <!-- Link - caster wheel -->
    <link name="back_caster">
        <inertial>
            <mass value="1" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.002526666666667" ixy="0" ixz="0" iyy="0.002526666666667" iyz="0" izz="0.005"/>
        </inertial>

        <!-- Caster wheel support -->
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <sphere radius="0.035" />
            </geometry>
        </collision>
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <sphere radius="0.035" />
            </geometry>
            <material name="DarkBlue">
                <color rgba="0.2 0.2 0.4 1" />
            </material>
        </visual>

        <!-- Caster wheel -->
        <collision>
            <origin rpy="0 0 0" xyz="0 0 -0.025" />
            <geometry>
                <sphere radius="0.025" />
            </geometry>
        </collision>
        <visual>
            <origin rpy="0 0 0" xyz="0 0 -0.025" />
            <geometry>
                <sphere radius="0.025" />
            </geometry>
            <material name="Grey">
                <color rgba="0.6 0.6 0.6 1" />
            </material>
        </visual>
    </link>

    <!-- Joint - caster wheel2 -->
    <joint name="joint_chassis_caster_wheel_2" type="fixed">
        <origin rpy="0 0 0" xyz="0.1 0 -0.15" />
        <child link="front_caster" />
        <parent link="base_link" />
        <joint_properties damping="1.0" friction="0.0" />
    </joint>

    <!-- Link - caster wheel 2 -->
    <link name="front_caster">
        <inertial>
            <mass value="1" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.002526666666667" ixy="0" ixz="0" iyy="0.002526666666667" iyz="0" izz="0.005"/>
        </inertial>

        <!-- Caster wheel support -->
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0.0" />
            <geometry>
                <sphere radius="0.035" />
            </geometry>
        </collision>
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0.0" />
            <geometry>
                <sphere radius="0.035" />
            </geometry>
            <material name="DarkBlue">
                <color rgba="0.2 0.2 0.4 1" />
            </material>
        </visual>

        <!-- Caster wheel -->
        <collision>
            <origin rpy="0 0 0" xyz="0 0 -0.025" />
            <geometry>
                <sphere radius="0.025" />
            </geometry>
        </collision>
        <visual>
            <origin rpy="0 0 0" xyz="0 0 -0.025" />
            <geometry>
                <sphere radius="0.025" />
            </geometry>
            <material name="Grey">
                <color rgba="0.6 0.6 0.6 1" />
            </material>
        </visual>
    </link>

    <!-- base_footprint -->
    <joint name="base_footprint" type="fixed" >
        <origin rpy="0 0 0" xyz="0 0 -0.15" />
        <child link="basefootprint" />
        <parent link="base_link" />
        <joint_properties damping="1.0" friction="0.0" />
    </joint>

    <!-- basefootprint -->
    <link name="basefootprint" >
        <inertial>
            <mass value="0" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0" />
        </inertial>
    </link>

    <!--Laser_Joint-->
    <joint name="Laser_joint" type="fixed">
        <origin rpy="0 0 0" xyz="0.25 0 0.075" />
        <child link="laserscan" />
        <parent link="base_link" />
        <joint_properties damping="1.0" friction="0.0" />
    </joint>

    <link name="laserscan" >
        <inertial>
            <mass value="0" />
            <origin xyz="0 0 0" rpy="3.1415 0 0" />
            <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0" />
        </inertial>
    </link>
</robot>

    

    
    


